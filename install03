#!/bin/bash

termux-setup-storage

pkg install termux-am x11-repo -y
pkg install python nmap vulkan-tools termux-x11-nightly -y
pkg install git python -y
pkg install coreutils -y
pkg install socat -y
pip install objgraph
pip install aiofiles
pip install blessings
cd /sdcard/
wget https://github.com/mebabo1/menano/releases/download/Box/Box64Droid01.tar.gz
tar xf Box64Droid01.tar.gz
rm -rf Box64Droid01.tar.gz
cd /data/data/com.termux/files
rm -rf opt root
wget https://github.com/mebabo1/menano/releases/download/Box/box64.tar.xz
tar xf box64.tar.xz
rm -rf box64.tar.xz
wget https://github.com/mebabo1/menano/releases/download/Box/python.tar.gz
tar xf python.tar.gz
rm -rf python.tar.gz
cd
echo "run x11 and x44"
rm -rf install03
