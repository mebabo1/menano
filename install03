#!/bin/bash
termux-setup-storage
pkg install python nmap vulkan-tools termux-api -y
pkg install git python -y
cd /sdcard/
wget https://github.com/mebabo1/menano/releases/download/Box/Box64Droid01.tar.gz
tar xf Box64Droid01.tar.gz
rm -rf Box64Droid01.tar.gz
cd
wget https://github.com/mebabo1/menano/releases/download/Box/box64.tar.xz
tar xf box64.tar.xz -C /
rm -rf box64.tar.xz
echo "run x11"
rm -rf install03
