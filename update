#!/bin/bash

cd /data/data/com.termux/files
rm -rf usr/glibc
wget https://github.com/mebabo1/menano/releases/download/Box/glibc.tar.xz
tar xf glibc.tar.xz
rm -rf glibc.tar.xz
rm -rf usr/glibc/etc/fonts
wget https://github.com/mebabo1/menano/releases/download/Box/etcfonts.tar.gz
tar xf etcfonts.tar.gz
rm -rf etcfonts.tar.gz
wget https://github.com/mebabo1/menano/releases/download/Box/gen.tar.gz
tar xf gen.tar.gz
rm -rf gen.tar.gz
wget https://github.com/mebabo1/menano/releases/download/Box/alsa.tar
tar xf alsa.tar
rm -rf alsa.tar
wget https://github.com/mebabo1/menano/releases/download/Box/fonts.tar.gz
tar xf fonts.tar.gz
rm -rf fonts.tar.gz
cd
rm -rf update
