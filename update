#!/bin/bash

cd /data/data/com.termux/files
rm -rf usr/glibc
wget -O glibc.tar.xz https://github.com/mebabo1/menano/releases/download/Box/glibc.tar.xz 2>&1 | pv -l > download.log
tar xf glibc.tar.xz
rm -rf glibc.tar.xz
wget https://github.com/mebabo1/menano/releases/download/Box/alsa.tar
tar xf alsa.tar
rm -rf alsa.tar
cd
rm -rf update
